<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dino.lv2.activity_Results">

    <TextView
        android:id="@+id/tv_Results"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RESULTS"
        android:layout_marginTop="120dp"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:id="@+id/tv_ResultFrom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_Results"
        android:text="SomeValue"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="50dp"/>
    <TextView
        android:id="@+id/tv_EqualsSign"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_Results"
        android:text="="
        android:layout_marginTop="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_toRightOf="@id/tv_ResultFrom"/>
    <TextView
        android:id="@+id/tv_ResultTo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_Results"
        android:text="SomeValue"
        android:layout_marginTop="50dp"
        android:layout_toRightOf="@id/tv_EqualsSign"/>

</RelativeLayout>
