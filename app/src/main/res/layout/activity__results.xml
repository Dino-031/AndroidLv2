<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_ResultsRootLayout"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dino.lv2.activity_Results"
    android:background="@color/colorResultBlue">

    <TextView
        android:id="@+id/tv_ValueFrom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VRIJEDNOST"
        android:layout_marginTop="120dp"
        android:layout_centerHorizontal="true"
        android:textColor="@color/colorWhite"/>

    <TextView
        android:id="@+id/tv_UnitFrom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_ValueFrom"
        android:text="Unit"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:textColor="@color/colorWhite"/>
    <TextView
        android:id="@+id/tv_ValueTo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_UnitFrom"
        android:text="Vrijednost"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:textColor="@color/colorWhite"/>
    <TextView
        android:id="@+id/tv_UnitTo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_ValueTo"
        android:text="Unit"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:textColor="@color/colorWhite"/>

</RelativeLayout>
